{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "semi": 0,
        "indent":"off",
        "no-param-reassign": 0,
        "import/extensions":0,
        "max-classes-per-file": ["error", 2]
    }
}
