{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "semi":0,
        "linebreak-style":0,
        "indent":"off",
        "import/extensions": 0,
        "no-var":"off"
    }
}
